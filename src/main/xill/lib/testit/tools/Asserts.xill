use Assert;

function error(message) {
    Assert.error(message);
}

function equal(actual, expected) {
    equal(actual, expected, null);
}

function equal(actual, expected, message) {
    Assert.equal(actual, expected, message);
}

function notEqual(actual, expected) {
    notEqual(actual, expected, null);
}

function notEqual(actual, expected, message) {
    Assert.notEqual(actual, expected, message);
}

function isFalse(actual) {
    isFalse(actual, null);
}

function isFalse(actual, message) {
    Assert.isFalse(actual, message);
}

function isTrue(actual) {
    isTrue(actual, null);
}

function isTrue(actual, message) {
    Assert.isTrue(actual, message);
}

function isNull(actual) {
    isNull(actual, null);
}

function isNull(actual, message) {
    Assert.isNull(actual, message);
}

function notNull(actual) {
    notNull(actual, null);
}

function notNull(actual, message) {
    Assert.notNull(actual, message);
}
