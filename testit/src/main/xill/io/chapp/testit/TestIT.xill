use Collection;
include io.chapp.testit.SuitesManager;
include io.chapp.testit.Run;
include io.chapp.testit.tools.Asserts;


function runAllSuites() {
    var result = runAllSuites(getSuites());
    if(Collection.length(result.suites.failed)) {
        error("There are test failures!");
    }
}